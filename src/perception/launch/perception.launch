<?xml version="1.0"?>
<launch>
    <node name="rosbag" pkg="rosbag" type="play" args="$(find perception)/dataset/two_cam.bag" />
    <node name="perception" pkg="perception" type="perception" output="screen">
        <param name="image_topic_prefix" value="/carla/camera_"/>
